#1
for n in range(10,100):
  if n%7==0:
   print(n, end=" ")
   
    
#2
print()
lista=["interdyscyplinarne", "Centrum", "Modelowania"]   
for l in lista:
  
  print (l[0],"-",l[-1:]," (",len(l),")")

#3
lista=["Ala","Ma","Kota"]

for slowo in lista:
  rslowo=slowo[::-1]
  for litera in rslowo:
    print(litera, end="")
  print() 
  
